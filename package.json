{
    "name": "kingdomarchitect",
    "version": "1.0.0",
    "description": "Kingdom architects is intended to be a building/management game",
    "type": "module",
    "scripts": {
        "build": "tsc && rollup --config rollup.config.js",
        "start": "cd public && static-server",
        "test": "tsc && node --test build/test/",
        "coverage": "c8 node --test && c8 report && open coverage/lcov-report/index.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/trymnilsen/kingdomarchitect.git"
    },
    "author": "Trym Nilsen",
    "private": true,
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/trymnilsen/kingdomarchitect/issues"
    },
    "overrides": {
        "rollup": "^3.5.0"
    },
    "homepage": "https://github.com/trymnilsen/kingdomarchitect#readme",
    "dependencies": {
        "@types/node": "^18.11.9",
        "c8": "^7.12.0",
        "rollup": "^3.5.0",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "static-server": "^2.2.1",
        "typescript": "^4.9.3"
    }
}

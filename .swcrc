{
    "test": ".*\\.ts$",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": false,
        "dynamicImport": false
      },
      "target": "es2020"
    },
    "module": {
      "type": "es6",
  
      // These are defaults.
      "strict": false,
      "strictMode": true,
      "lazy": false,
      "importInterop": "swc"
    }
  }
  
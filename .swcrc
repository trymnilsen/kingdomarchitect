{
    "test": ".*\\.ts$",
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": false,
        "dynamicImport": false
      },
      "target": "es2022"
    },
    "module": {
      "type": "es6",
  
      // These are defaults.
      "strict": false,
      "strictMode": true,
      "lazy": false
    },
    "sourceMaps": true
  }
  